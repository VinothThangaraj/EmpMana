  </configSections>
  <connectionStrings>
    <add name="EmployeeEntities" connectionString="metadata=res://*/EmpEntities.csdl|res://*/EmpEntities.ssdl|res://*/EmpEntities.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=Datasource;initial catalog=Employee;persist security info=True;user id=sa;password=password;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>

  <appSettings>
    <!--UserLockout-->
    <add key="MaxLoginFailedCount" value="5" />
    <add key="owin:appStartup" value="EmpManagement.Startup" />
    <add key="owin:AutomaticAppStartup" value="true" />
    <add key="BaseURLForSwagger" value="http://localhost:8786" />
  </appSettings>
